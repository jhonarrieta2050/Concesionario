@startuml
Vehiculo <|-- Automoviles
Vehiculo *-- Accesorios
Usuario o-- Vehiculo
class Usuario{
       - String nombre
       - int DNI;
       - String Direccion
       - int telefono
       - boolean vendedor
       -Vehiculo vehiculos
}

abstract class Vehiculo{
        # String marca
        # String modelo
        # String placa
        # String cilindrado
        # double precio
        # HashSet<Accesorios> adicionales

        + void agregarAdicionales(Accesorios accesorios)
}

class Automoviles{
    + void sonidoMotor()
}

class Accesorios{
    # String nombre;
    # String descripcion;


}
@enduml